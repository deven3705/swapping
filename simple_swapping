#include<iostream>
using namespace std;
void swap();
int main()
{
int n1,n2;

cout << "ENter n1: ";
cin >> n1;
cout << "Enter n2: ";
cin >> n2;

cout << "Before swaping : " << endl;
cout <<"n1=" << n1 << endl;
cout << "n2=" << n2 << endl;

swap(n1,n2);
cout << "After swaping : " << endl;
cout << "n1=" << n1 << endl; 
cout << "n2=" << n2 << endl; 
 
return 0;
}
void swap(int a,int b)
{
int temp =a;
a=b;
b=temp;
}

