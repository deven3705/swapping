#include<iostream>
using namespace std;

class B;
class A{
    int arr1[5]={2,3,4,5,6};
    public:
    friend void swap(A &,B &);
};

class B{
    int arr2[5]={6,5,4,3,7};
    public:
    friend void swap(A &,B &);
};

void swap( A &a1,B &b1){

    int size=(sizeof(a1.arr1)/sizeof(int));

    for(int i=0;i<size;i++){
        int temp=0;
        temp=a1.arr1[i];
        a1.arr1[i]=b1.arr2[i];
        b1.arr2[i]=temp;

    }
     cout<<"after swapping "<<endl;
     cout<<"value of class A arrays is :"<<endl;
     for(int i=0;i<size;i++){
        cout<<a1.arr1[i];
     }

     cout<<"value of class B arrays is :"<<endl;
     for(int i=0;i<size;i++){
        cout<<b1.arr2[i];
     }
     }
int main()
{
    A a1;
    B b1;
    swap(a1,b1);
    return 0;
return 0;
}
